    "from": "java:openjdk-7-jre",
    "maintainer": "Barnaby Gray <barnaby@pickle.me.uk>",
    "env": {
      "ES_VERSION": "2.0.0-beta2"
    },
    "cmd": "[\"elasticsearch/bin/elasticsearch\"]"
  },
