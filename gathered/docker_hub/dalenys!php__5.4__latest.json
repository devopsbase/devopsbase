      "curl -Ls -o /usr/local/bin/phpunit.phar https://phar.phpunit.de/phpunit.phar &&     chmod 755 /usr/local/bin/phpunit.phar &&     ln -s /usr/local/bin/phpunit.phar /usr/local/bin/phpunit",
      "php-choose-version 5.4",
      "apt-clean"
    ],
