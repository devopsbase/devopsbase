  "name": "h3xby/haproxy-proxylist Docker container",
  "description": "Haproxy with config autogenerated from simple proxies txt file",
  "readme": "# Haproxy with config autogenerated from simple proxies txt file\n\n## Config environment variables(ordered by priority)\n\n- PROXY_FILE_URL - url for download proxies txt list\n- PROXY_FILE - txt file with proxies(one proxy per line in format ip:port)\n",
  "dockerfile": "FROM haproxy:latest\n\nRUN apt-get update \\\n  && apt-get install -y --no-install-recommends curl ca-certificates cron \\\n  && apt-get clean -y && rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*\n\nADD proxyfile-generate.sh /proxyfile-generate.sh\nADD proxy-list.sh /proxy-list.sh\nRUN ln -s /proxyfile-generate.sh /etc/cron.daily/proxyfile-generate.sh\n\nENTRYPOINT [\"/proxy-list.sh\"]\nCMD [\"haproxy\", \"-f\", \"/usr/local/etc/haproxy/haproxy.cfg\"]\n",
  "dockerfile_json": {
    "add": [
      {
        "source": "proxyfile-generate.sh",
        "dest": "/proxyfile-generate.sh"
      },
      {
        "source": "proxy-list.sh",
        "dest": "/proxy-list.sh"
      }
    ],
    "expose": [],
    "volume": [],
    "run": [
      "apt-get update   && apt-get install -y --no-install-recommends curl ca-certificates cron   && apt-get clean -y && rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*",
      "ln -s /proxyfile-generate.sh /etc/cron.daily/proxyfile-generate.sh"
    ],
    "workdir": [],
    "from": "haproxy:latest",
    "entrypoint": "[\"/proxy-list.sh\"]",
    "cmd": "[\"haproxy\", \"-f\", \"/usr/local/etc/haproxy/haproxy.cfg\"]"
  },
