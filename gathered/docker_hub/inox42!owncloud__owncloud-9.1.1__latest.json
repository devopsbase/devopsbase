  "name": "inox42/owncloud Docker container",
  "description": "owncloud docker image",
  "readme": "# docker-owncloud\nMy owncloud docker image\n\nBased on inox42/php-fpm.\n",
  "dockerfile": "FROM inox42/php-fpm:nginx-1.11.2\nMAINTAINER inox42\n\n# Clear base directory\nRUN rm -rf /data/html/*\n\n# Install packages\nRUN apt-get update && \\\n        apt-get install -y smbclient php5-apcu && \\\n        apt-get clean\n\n# Install ownCloud\nRUN gpg --keyserver ha.pool.sks-keyservers.net --recv-keys E3036906AD9F30807351FAC32D5D5E97F6978A26\nENV OWNCLOUD_VERSION 9.1.1\nRUN curl -fsSL -o owncloud.tar.bz2 \"https://download.owncloud.org/community/owncloud-${OWNCLOUD_VERSION}.tar.bz2\" && \\\n        curl -fsSL -o owncloud.tar.bz2.asc \"https://download.owncloud.org/community/owncloud-${OWNCLOUD_VERSION}.tar.bz2.asc\" && \\\n        gpg --verify owncloud.tar.bz2.asc && \\\n        tar -xjf owncloud.tar.bz2 -C /usr/src/ && \\\n        rm owncloud.tar.bz2 owncloud.tar.bz2.asc\n\n#Â Volume\nRUN mkdir /data/owncloud\nVOLUME /data/owncloud\nRUN mkdir /data/tmp\nVOLUME /data/tmp\n\n# Configure php5-fpm\nRUN echo \"env[HOSTNAME] = \\$HOSTNAME\" >> /etc/php5/fpm/php-fpm.conf\nRUN echo \"env[PATH] = /usr/local/bin:/usr/bin:/bin\" >> /etc/php5/fpm/php-fpm.conf\nRUN echo \"env[TMP] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf\nRUN echo \"env[TMPDIR] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf\nRUN echo \"env[TEMP] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf\n\n# Configure ownCloud\nRUN rm /etc/nginx/host.d/*.conf\nRUN rm /etc/nginx/conf.d/*.conf\nADD etc /etc\nADD data /data\n\n# Update php5-fpm to accept big file\nRUN sed -i 's/upload_max_filesize.*/upload_max_filesize = 16G/g' /etc/php5/fpm/php.ini\nRUN sed -i 's/post_max_size.*/post_max_size = 16G/g' /etc/php5/fpm/php.ini\nRUN sed -i 's/output_buffering.*/output_buffering = 0/g' /etc/php5/fpm/php.ini\n\n# Autorize owncloud to change max file size\nRUN chown 777 /usr/src/owncloud/.htaccess\nRUN chown 777 /usr/src/owncloud/.user.ini\nRUN sed -i 's/513M/16G/g' /usr/src/owncloud/.htaccess\nRUN sed -i 's/513M/16G/g' /usr/src/owncloud/.user.ini\n\n# Environnement\nENV OWNCLOUD_SSL \"NO\"\nENV NGINX_SSL_CRT \"/data/cert/owncloud.crt\"\nENV NGINX_SSL_KEY \"/data/cert/owncloud.key\"\nENV NGINX_SSL_DOCKER_PORT 443\n\n# Port\nEXPOSE 80 443\n\n# Entrypoint\nENTRYPOINT [\"/data/init/init.sh\"]\n",
  "dockerfile_json": {
    "add": [
      {
        "source": "etc",
        "dest": "/etc"
      },
      {
        "source": "data",
        "dest": "/data"
      }
    ],
    "expose": [
      80
    ],
    "volume": [
      "/data/owncloud",
      "/data/tmp"
    ],
    "run": [
      "rm -rf /data/html/*",
      "apt-get update &&         apt-get install -y smbclient php5-apcu &&         apt-get clean",
      "gpg --keyserver ha.pool.sks-keyservers.net --recv-keys E3036906AD9F30807351FAC32D5D5E97F6978A26",
      "curl -fsSL -o owncloud.tar.bz2 \"https://download.owncloud.org/community/owncloud-${OWNCLOUD_VERSION}.tar.bz2\" &&         curl -fsSL -o owncloud.tar.bz2.asc \"https://download.owncloud.org/community/owncloud-${OWNCLOUD_VERSION}.tar.bz2.asc\" &&         gpg --verify owncloud.tar.bz2.asc &&         tar -xjf owncloud.tar.bz2 -C /usr/src/ &&         rm owncloud.tar.bz2 owncloud.tar.bz2.asc",
      "mkdir /data/owncloud",
      "mkdir /data/tmp",
      "echo \"env[HOSTNAME] = \\$HOSTNAME\" >> /etc/php5/fpm/php-fpm.conf",
      "echo \"env[PATH] = /usr/local/bin:/usr/bin:/bin\" >> /etc/php5/fpm/php-fpm.conf",
      "echo \"env[TMP] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf",
      "echo \"env[TMPDIR] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf",
      "echo \"env[TEMP] = /data/tmp\" >> /etc/php5/fpm/php-fpm.conf",
      "rm /etc/nginx/host.d/*.conf",
      "rm /etc/nginx/conf.d/*.conf",
      "sed -i 's/upload_max_filesize.*/upload_max_filesize = 16G/g' /etc/php5/fpm/php.ini",
      "sed -i 's/post_max_size.*/post_max_size = 16G/g' /etc/php5/fpm/php.ini",
      "sed -i 's/output_buffering.*/output_buffering = 0/g' /etc/php5/fpm/php.ini",
      "chown 777 /usr/src/owncloud/.htaccess",
      "chown 777 /usr/src/owncloud/.user.ini",
      "sed -i 's/513M/16G/g' /usr/src/owncloud/.htaccess",
      "sed -i 's/513M/16G/g' /usr/src/owncloud/.user.ini"
    ],
    "workdir": [],
    "from": "inox42/php-fpm:nginx-1.11.2",
    "maintainer": "inox42",
    "env": {
      "OWNCLOUD_VERSION": "9.1.1",
      "OWNCLOUD_SSL": "\"NO\"",
      "NGINX_SSL_CRT": "\"/data/cert/owncloud.crt\"",
      "NGINX_SSL_KEY": "\"/data/cert/owncloud.key\"",
      "NGINX_SSL_DOCKER_PORT": "443"
    },
    "entrypoint": "[\"/data/init/init.sh\"]"
  },
